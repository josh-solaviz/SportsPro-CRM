@model TechnicianViewModel
@{ ViewBag.Title = "Technician";}

@* Razor code block to specify a section *@
@section scripts {
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
}

<h1> @ViewBag.Title </h1>
<form asp-action="Get">
<div asp-validation-summary="All" class="text-danger"></div>
<div class="form-group">
<select name="SelectedItem" asp-for="Name"
        asp-items="@(Model.TechnicianList)"
        class="form-control">
    <option value="">Select a Technician</option>
</select>
</div>
<br>
<button type="submit" asp-action="SetTechnician" asp-route-id="@Model.CurrentTechnician.TechnicianID" class="btn btn-primary" style="box-shadow: 0px 0px 3px 0px black">Select</button>
</form>







